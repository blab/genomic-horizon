	Issn = {0169-5347},
	Journal = {Trends in Ecology \& Evolution},
	Keywords = {bacteria, DNA virus, epidemiological models, evolutionary rate, infectious disease, phylodynamics},
	Month = jun,
	Number = {6},
	Pages = {306--313},
	Title = {Measurably evolving pathogens in the genomic era},
