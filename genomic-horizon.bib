	Issn = {0022-538X, 1098-5514},
	Journal = {Journal of Virology},
	Language = {en},
	Month = apr,
	Number = {8},
	Pages = {4330--4341},
	Pmid = {15047846},
	Title = {Rapid {Diagnosis} of {Ebola} {Hemorrhagic} {Fever} by {Reverse} {Transcription}-{PCR} in an {Outbreak} {Setting} and {Assessment} of {Patient} {Viral} {Load} as a {Predictor} of {Outcome}},
