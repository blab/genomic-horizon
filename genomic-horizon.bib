	Issn = {1931-3128},
	Journal = {Cell Host \& Microbe},
	Month = dec,
	Number = {6},
	Pages = {659--669},
	Title = {Evolution and {Spread} of {Ebola} {Virus} in {Liberia}, 2014--2015},
