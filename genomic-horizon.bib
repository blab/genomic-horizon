	Doi = {10.1371/journal.ppat.1003932},
	File = {PubMed Central Full Text PDF:/Users/evogytis/Zotero/storage/QHXDJLAB/Lemey et al. - 2014 - Unifying Viral Genetics and Human Transportation D.pdf:application/pdf},
	Issn = {1553-7366},
