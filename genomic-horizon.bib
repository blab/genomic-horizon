	Doi = {10.1128/JVI.03278-12},
	File = {Full Text PDF:/Users/evogytis/Zotero/storage/V757MP32/Tan et al. - 2013 - The Comparative Genomics of Human Respiratory Sync.pdf:application/pdf;Snapshot:/Users/evogytis/Zotero/storage/DDH3UGFV/8213.html:text/html},
	Issn = {0022-538X, 1098-5514},
	Journal = {Journal of Virology},
	Language = {en},
