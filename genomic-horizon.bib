	Number = {14},
	Pages = {8213--8226},
	Pmid = {23698290},
	Shorttitle = {The {Comparative} {Genomics} of {Human} {Respiratory} {Syncytial} {Virus} {Subgroups} {A} and {B}},
	Title = {The {Comparative} {Genomics} of {Human} {Respiratory} {Syncytial} {Virus} {Subgroups} {A} and {B}: {Genetic} {Variability} and {Molecular} {Evolutionary} {Dynamics}},
